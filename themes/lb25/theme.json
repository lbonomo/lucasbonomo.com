{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 3,
  "settings": {
    "appearanceTools": true,
    "color": {
      "defaultDuotone": false,
      "defaultPalette": false,
      "defaultGradients": false,
      "custom": false,
      "palette": [
        {
          "slug": "light-blue",
          "color": "#1E3A8A",
          "name": "Light Blue"
        },
        {
          "slug": "dark-blue",
          "color": "rgb(37, 99, 235)",
          "name": "Dark Blue"
        },
        {
          "slug": "secondary",
          "color": "#6B7280",
          "name": "Secondary Gray"
        },
        {
          "slug": "white",
          "color": "#FFFFFF",
          "name": "White"
        },
        {
          "slug": "gray-600",
          "color": "rgb(75 85 99 / var(--tw-text-opacity, 1))",
          "name": "Gray 600"
        },
        {
          "slug": "gray-500",
          "color": "rgb(107 114 128 / var(--tw-text-opacity, 1))",
          "name": "Gray 500"
        },
        {
          "slug": "background",
          "color": "#F9FAFB",
          "name": "Background"
        },
        {
          "slug": "blue-accent",
          "color": "#3B82F6",
          "name": "Blue Accent"
        },
        {
          "slug": "dark",
          "color": "#111827",
          "name": "Dark"
        },
        {
          "slug": "light-gray",
          "color": "#E5E7EB",
          "name": "Light Gray"
        },
        {
          "slug": "orange",
          "color": "#F59E0B",
          "name": "Orange"
        }
      ]
    },
    "typography": {
      "customFontSize": false,
      "defaultFontSizes": false,
      "fontSizes": [
        {
          "name": "Extra Small",
          "slug": "x-small",
          "size": "0.75rem"
        },
        {
          "name": "Small",
          "slug": "small",
          "size": "0.875rem"
        },
        {
          "name": "Medium",
          "slug": "medium",
          "size": "1rem"
        },
        {
          "name": "Large",
          "slug": "large",
          "size": "1.125rem"
        },
        {
          "name": "Extra Large",
          "slug": "x-large",
          "size": "1.5rem"
        },
        {
          "name": "XX Large",
          "slug": "xx-large",
          "size": "2rem"
        },
        {
          "name": "XXX Large",
          "slug": "xxx-large",
          "size": "3.75rem"
        }
      ],
      "fontStyle": true,
      "fontWeight": true,
      "letterSpacing": true,
      "lineHeight": true,
      "textDecoration": true,
      "textTransform": true,
      "dropCap": true,
      "fontFamilies": [
        {
          "fontFamily": "Inter, Arial, sans-serif",
          "slug": "primary",
          "name": "Primary Font",
          "fontFace": [
            {
              "fontFamily": "Inter",
              "fontWeight": "100 900",
              "fontStyle": "normal",
              "src": [
                "file:./assets/fonts/InterVariable.woff2"
              ]
            }
          ]
        },
        {
          "fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen, Ubuntu, Cantarell, 'Open Sans', sans-serif",
          "slug": "system",
          "name": "System Font"
        }
      ]
    },
    "spacing": {
      "margin": true,
      "padding": true,
      "units": [
        "px",
        "%",
        "em",
        "rem",
        "vw",
        "vh"
      ],
      "customSpacingSize": false,
      "spacingSizes": [
        {
          "name": "Extra Small",
          "slug": "10",
          "size": "0.25rem"
        },
        {
          "name": "Small",
          "slug": "20",
          "size": "0.5rem"
        },
        {
          "name": "Medium",
          "slug": "30",
          "size": "1rem"
        },
        {
          "name": "Large",
          "slug": "40",
          "size": "1.5rem"
        },
        {
          "name": "Extra Large",
          "slug": "50",
          "size": "2rem"
        },
        {
          "name": "XX Large",
          "slug": "60",
          "size": "3rem"
        }
      ]
    },
    "layout": {
      "contentSize": "800px",
      "wideSize": "1200px"
    },
    "useRootPaddingAwareAlignments": false
  },
  "styles": {
    "spacing": {
      "blockGap": "0"
    },
    "typography": {
      "fontFamily": "var(--wp--preset--font-family--primary)",
      "fontSize": "var(--wp--preset--font-size--medium)"
    },
    "color": {
      "background": "var(--wp--preset--color--background)",
      "text": "var(--wp--preset--color--text)"
    },
    "elements": {
      "h1": {
        "spacing": {
          "margin": {
            "top": "0",
            "bottom": "0"
          }
        },
        "typography": {
          "fontSize": "var(--wp--preset--font-size--xxx-large)",
          "fontWeight": "bold"
        },
        "color": {
          "text": "var(--wp--preset--color--primary)"
        }
      },
      "h2": {
        "spacing": {
          "margin": {
            "top": "0",
            "bottom": "0"
          }
        },
        "typography": {
          "fontSize": "var(--wp--preset--font-size--xx-large)",
          "fontWeight": "bold"
        },
        "color": {
          "text": "var(--wp--preset--color--primary)"
        }
      },
      "h3": {
        "spacing": {
          "margin": {
            "top": "0",
            "bottom": "0"
          }
        },
        "typography": {
          "fontSize": "var(--wp--preset--font-size--x-large)",
          "fontWeight": "bold"
        },
        "color": {
          "text": "var(--wp--preset--color--secondary)"
        }
      },
      "h4": {
        "spacing": {
          "margin": {
            "top": "0",
            "bottom": "0"
          }
        },
        "typography": {
          "fontSize": "var(--wp--preset--font-size--large)",
          "fontWeight": "bold"
        },
        "color": {
          "text": "var(--wp--preset--color--secondary)"
        }
      },
      "h5": {
        "spacing": {
          "margin": {
            "top": "0",
            "bottom": "0"
          }
        },
        "typography": {
          "fontSize": "var(--wp--preset--font-size--medium)",
          "fontWeight": "bold"
        },
        "color": {
          "text": "var(--wp--preset--color--text)"
        }
      },
      "h6": {
        "spacing": {
          "margin": {
            "top": "0",
            "bottom": "0"
          }
        },
        "typography": {
          "fontSize": "var(--wp--preset--font-size--small)",
          "fontWeight": "bold"
        },
        "color": {
          "text": "var(--wp--preset--color--text)"
        }
      },
      "button": {
        "color": {
          "background": "var(--wp--preset--color--dark-blue)",
          "text": "var(--wp--preset--color--white)"
        },
        "typography": {
          "fontSize": "var(--wp--preset--font-size--large)",
          "fontWeight": "bold"
        },
        "border": {
          "radius": "0.5rem"
        },
        "spacing": {
          "padding": {
            "top": "1rem",
            "bottom": "1rem",
            "left": "2rem",
            "right": "2rem"
          }
        }
      },
      "link": {
        "color": {
          "text": "var(--wp--preset--color--dark-blue)"
        },
        "typography": {
          "textDecoration": "none"
        }
      }
    },
    "blocks": {
      "core/paragraph": {
        "spacing": {
          "margin": {
            "top": "0",
            "bottom": "0"
          }
        }
      },
      "core/heading": {
        "spacing": {
          "margin": {
            "top": "0",
            "bottom": "0"
          }
        }
      },
      "core/group": {
        "spacing": {
          "margin": {
            "top": "0",
            "bottom": "0"
          }
        }
      },
      "core/columns": {
        "spacing": {
          "margin": {
            "top": "0",
            "bottom": "0"
          }
        }
      },
      "core/column": {
        "spacing": {
          "margin": {
            "top": "0",
            "bottom": "0"
          }
        }
      },
      "core/cover": {
        "spacing": {
          "margin": {
            "top": "0",
            "bottom": "0"
          }
        },
        "variations": {
          "hero": {
            "spacing": {
              "margin": {
                "top": "0",
                "bottom": "0"
              },
              "padding": {
                "top": "8rem",
                "bottom": "8rem",
                "left": "2rem",
                "right": "2rem"
              }
            }
          }
        }
      }
    }
  }
}